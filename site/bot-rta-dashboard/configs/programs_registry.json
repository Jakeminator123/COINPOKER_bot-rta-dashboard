  "category_definitions": {
    "bots": {
      "name": "Automated Play / Bot Software",
      "description": "Programs that automatically play poker without human intervention. All forms of auto-decisions/auto-actions are prohibited.",
      "default_points": 15
    },
    "rta_tools": {
      "name": "Real-Time Assistance / Solvers",
      "description": "Tools that provide real-time advice during active hands (prohibited during play). Includes GTO solvers, RTA overlays, and training tools with live assistance.",
      "default_points": 10
    },
    "automation": {
      "name": "Automation Tools",
      "description": "Scripting engines and automation frameworks that can be used to create bots or automate actions. Includes Python, AutoHotkey, AutoIt, and RPA tools.",
      "default_points": 10
    },
    "macros": {
      "name": "Macro Tools",
      "description": "Macro recording and playback tools that can automate mouse/keyboard actions. All auto-clickers and macro tools are prohibited.",
      "default_points": 10
    },
    "hud_tracking": {
      "name": "Dynamic HUDs / Screen-Scraping Tools",
      "description": "Heads-up displays and tracking software that read live tables or provide dynamic overlays during play. Prohibited if they update mid-hand or scrape live screen data.",
      "default_points": 10
    },
    "communication": {
      "name": "Communication Apps",
      "description": "Messaging and communication applications that could be used for coordination or receiving assistance during play.",
      "default_points": 5
    },
    "data_mining": {
      "name": "Data Mining / Hand History Sharing",
      "description": "Services that provide hand histories you did not play yourself or unauthorized data pools. Prohibited for creating unfair advantages.",
      "default_points": 10
    }
  }
}